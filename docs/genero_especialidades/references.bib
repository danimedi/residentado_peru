  journal = {Nature Communications},
  keywords = {COVID-19,Female,Humans,Intensive Care Units,Male,Mortality,Pandemics,Risk Factors,SARS-CoV-2,Sex Factors},
  language = {eng},
  number = {1},
  pmcid = {PMC7726563},
